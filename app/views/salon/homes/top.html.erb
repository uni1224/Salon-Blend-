<%= link_to "ログアウト", destroy_salon_session_path, method: :delete %>
 <% if salon_signed_in?%>
<div class="item-index-outpatient">
<%= Time.now.strftime('%Y/%m/%d')%>の予約者リスト
<table class="table">
  <thead>
    <tr>
      <th scope="col">予約時間</th>
      <th scope="col">名前</th>
    </tr>
  </thead>
    <% @reserve.each do |reserve|%>
  <tbody>
    <tr>
      <td><%= reserve.time%></td>
      <td><%= link_to reserve.user.user_name,user_path(reserve.user.id)%></td>
    </tr>
  </tbody>
  <%end%>
</table>
</div>
<%end%>