<div class="container">
  <div class="row mt-4">
    <div class="col-md-3 col-sm-3">
       <h4 class="ml-5 mb-5"><span class="title-custom px-4">メニュー一覧</span></h4>

        </div>
         <table class="table table-striped">
           <tr>
             <th>メニューID</th>
             <th>メニュー名</th>
             <th>メニュー料金</th>
           </tr>
               <% @menus.each do |menu| %>
           <tr>
             <td><%= menu.id %></td>
             <td><%= link_to menu.title, salon_menu_path(menu) %></td>
             <td>￥<%#= menu.tax_price.to_s(:delimited) %></td>
           </tr>
               <%end%>

         </table>

   </div>
</div>


